    "eslint": "5.16.0",
    "eslint-plugin-jsdoc": "4.8.4",
    "eslint-plugin-lodash": "5.1.0",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-security": "1.4.0",
    "lodash": "4.17.11",
    "mocha": "6.1.4",
    "newman": "4.4.1",
    "nsp": "3.2.1",
    "nyc": "14.1.1",
