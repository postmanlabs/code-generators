    "eslint": "4.19.1",
    "eslint-plugin-jsdoc": "3.5.0",
    "eslint-plugin-lodash": "2.6.1",
    "eslint-plugin-mocha": "4.12.1",
    "eslint-plugin-security": "1.4.0",
    "lodash": "4.17.5",
    "mocha": "5.0.5",
    "newman": "3.9.3",
    "nsp": "2.8.0",
    "nyc": "11.6.0",
